<Window x:Class="WPFClinica.Windows.CriarProcedimentoLab"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFClinica.Windows"
        mc:Ignorable="d"
        Title="Criar Procedimento Laboratorial" Height="150" Width="400">
    <Grid>
        <StackPanel Margin="5">
            <TextBlock Foreground="Gray">Nome</TextBlock>
            <TextBox FontSize="15" Name="NomeTB"></TextBox>
            <Button Padding="10,2,10,2" FontSize="15" Margin="0,10,0,0" Name="CriarB" Click="CriarB_Click">Criar</Button>
        </StackPanel>
    </Grid>
</Window>
