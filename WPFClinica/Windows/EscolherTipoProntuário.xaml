<Window x:Class="WPFClinica.Windows.EscolherTipoProntuário"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFClinica.Windows"
        mc:Ignorable="d" Title="EscolherTipoProntuário" Height="200" Width="300" WindowStartupLocation="CenterScreen" WindowStyle="ToolWindow">
    <Grid>
        <Grid.Resources>
            <Style TargetType="{x:Type Button}" BasedOn="{StaticResource {x:Type Button}}">
                <Setter Property="Margin" Value="10"></Setter>
            </Style>
        </Grid.Resources>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button x:Name="WordDocumentB" Click="WordDocumentB_Click">Documento word</Button>
            <Button x:Name="WindowB" Click="WindowB_Click">Janela</Button>
        </StackPanel>
    </Grid>
</Window>
