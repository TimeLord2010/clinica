<Window x:Class="WPFClinica.Windows.EditarHistorico"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFClinica.Windows"
        mc:Ignorable="d"
        Title="Editar histórico" Height="150" Width="200" WindowStyle="ToolWindow">
    <Grid>
        <StackPanel Margin="10">
            <CheckBox FontSize="15" Margin="0,0,0,10" Name="PagoChB">Pago</CheckBox>
            <CheckBox FontSize="15" Margin="0,0,0,10" Name="RecebidoChB">Recebido</CheckBox>
            <Button FontSize="15" Padding="6,2,6,2" Name="SalvarB" Click="SalvarB_Click">Salvar</Button>
        </StackPanel>
    </Grid>
</Window>
