<Window x:Class="WPFClinica.Windows.EscolherEspecializacao"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFClinica.Windows"
        mc:Ignorable="d"
        Title="Escolher Especializacao" Height="150" Width="300" WindowStyle="ToolWindow">
    <Grid>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
            <ComboBox Width="200" FontSize="15" SelectedIndex="0" x:Name="EspecializacaoCB">
                <ComboBoxItem>Test</ComboBoxItem>
            </ComboBox>
            <Button x:Name="OkB" FontSize="15" Margin="0,10,0,0" Click="OkB_Click">Ok</Button>
        </StackPanel>
    </Grid>
</Window>
